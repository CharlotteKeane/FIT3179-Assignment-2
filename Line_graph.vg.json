{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Trends in physical activity guideline adherence by sex, 2007–08 to 2022",
  "data": {"url": "Males.csv"},
  "width": 700,
  "height": 400,
  "title": {
    "text": "Physical Activity Guideline (Met vs Did Not Meet) by Sex, 2007–08 to 2022",
    "fontSize": 20,
    "anchor": "start"
  },
  "mark": {"type": "line", "point": true, "strokeWidth": 3},
  "encoding": {
    "x": {
      "field": "Year",
      "type": "ordinal",
      "title": "Survey Year"
    },
    "y": {
      "field": "Percent",
      "type": "quantitative",
      "title": "Percentage (%)"
    },
    "color": {
      "field": "Sex",
      "type": "nominal",
      "title": "Sex"
    },
    "strokeDash": {
      "field": "Category",
      "type": "nominal",
      "title": "Guideline Category"
    },
    "tooltip": [
      {"field": "Year", "title": "Year"},
      {"field": "Sex", "title": "Sex"},
      {"field": "Category", "title": "Category"},
      {"field": "Percent", "title": "Percent", "format": ".1f"}
    ]
  },
  "background": "#f0f8ff"
}
