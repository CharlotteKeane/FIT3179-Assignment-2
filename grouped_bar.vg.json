{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "description": "Grouped bar chart: Physical activity by age group.",
  "data": {
    "url": "Males.csv"
  },
  "transform": [
    {"fold": ["Met (%)", "Did not meet (%)"], "as": ["Status", "Value"]}
  ],
  "mark": {
    "type": "bar",
    "tooltip": true
  },
  "encoding": {
    "x": {
      "field": "Age group",
      "type": "ordinal",
      "axis": {"labelAngle": 0},
      "title": "Age Group"
    },
    "xOffset": {"field": "Status"},
    "y": {"field": "Value", "type": "quantitative", "title": "Percentage"},
    "color": {
      "field": "Status",
      "type": "nominal",
      "scale": {
        "domain": ["Met (%)", "Did not meet (%)"],
        "range": ["#1f77b4", "#ff7f0e"]
      },
      "title": "Physical Activity Status"
    },
    "tooltip": [
      {"field": "Age group", "type": "ordinal"},
      {"field": "Status", "type": "nominal"},
      {"field": "Value", "type": "quantitative", "format": ".1f"}
    ]
  }
}
